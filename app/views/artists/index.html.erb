<section class="container allartists">
  <div class="row text-center">
    <h1>Artists</h1>
  </div>

  <div class="row">
    <% @artists.each do |artist| %>
      <div class="col-md-4">
        <p><%= link_to image_tag(artist.logo, :class => "logo"), artist %></p>
      </div>
      <% end %>
    </div>
</section>
